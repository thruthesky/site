{
  "name": "site",
  "version": "0.0.1",
  "author": "Withcenter, Inc.",
  "homepage": "http://www.ontue.com/",
  "scripts": {
    "ng": "ng",
    "start": "ionic serve",
    "build": "ionic build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "cypress open --env server=local --config integrationFolder=src,testFiles=**/test.*.spec.js",
    "e2e:headless": "npm run e2e:headless:katalkenglish && npm run e2e:headless:ontue && npm run e2e:headless:englishas",
    "e2e:remote": "cypress open --env server=remote --config integrationFolder=src,testFiles=**/test.*.spec.js",
    "e2e:headless:katalkenglish": "cypress run --env server=local --config integrationFolder=src,testFiles=**/test.katalk*.spec.js",
    "e2e:headless:englishas": "cypress run --env server=local --config integrationFolder=src,testFiles=**/test.englishas*.spec.js",
    "e2e:headless:ontue": "cypress run --env server=local --config integrationFolder=src,testFiles=**/test.ontue*.spec.js",
    "e2e:remote:headless": "npm run e2e:remote:headless:katalkenglish && npm run e2e:remote:headless:ontue && npm run e2e:remote:headless:englishas",
    "e2e:remote:headless:katalkenglish": "cypress run --env server=remote --config integrationFolder=src,testFiles=**/test.katalk*.spec.js",
    "e2e:remote:headless:englishas": "cypress run --env server=remote --config integrationFolder=src,testFiles=**/test.englishas*.spec.js",
    "e2e:remote:headless:ontue": "cypress run --env server=remote --config integrationFolder=src,testFiles=**/test.ontue*.spec.js",
    "s": "ng serve --disable-host-check",
    "serve:local": "ng serve --disable-host-check --configuration=local-backend-server",
    "serve:hmr": "ng serve --disable-host-check --configuration=hmr",
    "seo": "ng build --watch --output-path=/Users/thruthesky/www/wordpress --delete-output-path=false",
    "bt": "ng build --prod --named-chunks=true",
    "publish": "npm run build:prod && npm run scp",
    "publish:gitpage": "ng build --prod --output-path docs --base-href=/site/ && git add --all && git commit -m 'buildtest' && git push",
    "publish:all": "npm run build:prod && npm run scp:all",
    "build:prod": "ng build --prod",
    "scp": "cd www && scp *.js *.css *.html ontue@ontue.com:./www && cd assets && scp -r * ontue@ontue.com:./www/assets",
    "scp:all": "cd www && scp -r * ontue@ontue.com:./www",
    "delete-remote-js": "ssh ontue@ontue.com 'rm -f www/*.js'",
    "ssl": "ng serve --disable-host-check --configuration=local-backend-server --ssl --ssl-key './tmp/ssl/wild-katalkenglish/private.key' --ssl-cert './tmp/ssl/wild-katalkenglish/certificate.crt' --port 9443",
    "ssl:live": "ng serve --disable-host-check --ssl --ssl-key './tmp/ssl/wild-katalkenglish/private.key' --ssl-cert './tmp/ssl/wild-katalkenglish/certificate.crt' --port 9443",
    "sssl": "ng serve --disable-host-check --configuration=local-backend-server --ssl --ssl-key ./tmp/ssl/wild-katalkenglish/private.key --ssl-cert ./tmp/ssl/wild-katalkenglish/certificate.crt --port 9443",
    "sssl:live": "ng serve --disable-host-check --ssl --ssl-key ./tmp/ssl/wild-katalkenglish/private.key --ssl-cert ./tmp/ssl/wild-katalkenglish/certificate.crt --port 9443"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.2.8",
    "@angular/cdk": "^7.3.4",
    "@angular/common": "^7.2.8",
    "@angular/core": "^7.2.8",
